@{
  Layout = "_Layout";
}

@using Factory.Models;
@model List<Factory.Models.Engineer>;

<h2>Search Results</h2>
<hr />

@if (@Model.Count == 0) 
{
  <p>No engineers matched!</p>
}
else
{
  @foreach (Engineer eng in Model)
  {
    <li>@Html.ActionLink($"{eng.EngineerName}", "Details", new {id = eng.EngineerId })</li>
  }
}
