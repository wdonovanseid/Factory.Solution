@{
  Layout = "_Layout";
}

@using Factory.Models;
@model List<Factory.Models.Machine>;

<h2>Search Results</h2>
<hr />

@if (@Model.Count == 0) 
{
  <p>No Machines matched!</p>
}
else
{
  @foreach (Machine mach in Model)
  {
    <li>@Html.ActionLink($"{mach.MachineName}", "Details", new {id = mach.MachineId })</li>
  }
}
